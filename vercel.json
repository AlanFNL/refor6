{
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/blog",
        "dest": "/src/components/pages/Home/index.jsx"
      },
      {
        "src": "/blogs/:id",
        "dest": "/src/components/pages/Blog/[id].js"
      },
      {
        "src": "/blogs",
        "dest": "/src/components/pages/Blog/index.jsx"
      },
      {
        "src": "/(.*)",
        "dest": "/src/app.jsx"
      }
    ]
  }
  